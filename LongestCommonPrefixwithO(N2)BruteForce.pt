    if not strs:
        return "";

    prefix = ""
    for i in range(len(strs[0])):
        refChar = strs[0][i]   # ya flower k f ko store kara ga

        for word in strs[1:]:
            if i >= len(word) or word[i] != refChar:
               return prefix;

        prefix += refChar;
    return prefix;


    



    



